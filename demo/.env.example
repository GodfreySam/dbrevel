# DbRevel Configuration
# Copy this file to .env and update with your values

# API Base URL (production: https://api.dbrevel.io)
DBREVEL_BASE_URL=https://api.dbrevel.io

# Project API Key
# Get this from your DbRevel dashboard under Project Settings
# For demo/testing, you can use: dbrevel_demo_project_key
DBREVEL_API_KEY=your-project-api-key-here
