# DbRevel Frontend Configuration
# Copy this file to .env.local and customize for your environment

# API Base URL (without trailing slash)
VITE_API_URL=http://localhost:8000/api/v1

# Account/Project API Key (for SaaS mode)
# Options:
#   - dbrevel_demo_project_key: Uses cloud-hosted demo project (default)
#   - Your custom project API key: For production use with your own project databases
VITE_ACCOUNT_KEY=dbrevel_demo_project_key

# Request timeout in milliseconds
VITE_TIMEOUT=30000

# Sentry Error Tracking (optional - leave empty to disable)
# Get your DSN from: https://sentry.io
VITE_SENTRY_DSN=
