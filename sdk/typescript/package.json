{
	"name": "@dbrevel/sdk",
	"version": "0.2.1",
	"description": "TypeScript SDK for DbRevel - AI-powered database SDK that converts natural language into secure, optimized queries for any database",
	"author": "Godfrey Samuel <godfreysam09@gmail.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GodfreySam/dbrevel.git",
		"directory": "sdk/typescript"
	},
	"homepage": "https://github.com/GodfreySam/dbrevel/tree/main/sdk/typescript#readme",
	"bugs": {
		"url": "https://github.com/GodfreySam/dbrevel/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/types/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/types/index.d.ts",
				"default": "./dist/esm/index.js"
			},
			"require": {
				"types": "./dist/types/index.d.ts",
				"default": "./dist/cjs/index.js"
			}
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"sideEffects": false,
	"engines": {
		"node": ">=16.0.0"
	},
	"scripts": {
		"build": "npm run build:cjs && npm run build:esm && npm run build:types",
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:esm": "tsc -p tsconfig.esm.json && node scripts/fix-esm-imports.cjs",
		"build:types": "tsc -p tsconfig.types.json",
		"dev": "tsc --watch",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"lint": "eslint src --ext .ts",
		"clean": "rm -rf dist",
		"prepublishOnly": "npm run clean && npm run build && npm test"
	},
	"keywords": [
		"dbrevel",
		"ai",
		"database",
		"sql",
		"mongodb",
		"postgresql",
		"natural-language",
		"query",
		"gemini",
		"nosql",
		"typescript",
		"sdk"
	],
	"devDependencies": {
		"@types/jest": "^29.5.0",
		"@types/node": "^20.10.0",
		"jest": "^29.7.0",
		"ts-jest": "^29.1.0",
		"typescript": "^5.3.0"
	},
	"dependencies": {}
}
